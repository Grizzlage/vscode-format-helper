{
	"name": "vscode-json-helper",
	"displayName": "vscode-json-helper",
	"description": "",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.79.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "json-helper.paste",
				"title": "Json: Paste"
			},{
				"command": "json-helper.format",
				"title": "Json: Format"
			},{
				"command": "json-helper.fold",
				"title": "Json: Fold",
				"when": "editorLangId == json"
			},{
				"command": "json-helper.unfold",
				"title": "Json: Unfold",
				"when": "editorLangId == json"
			},{
				"command": "json-helper.foldAll",
				"title": "Json: Fold All",
				"when": "editorLangId == json"
			},{
				"command": "json-helper.unfoldAll",
				"title": "Json: Unfold All",
				"when": "editorLangId == json"
			}
		],

		"menus": {
			"editor/context": [
				{
					"command": "json-helper.paste",
					"group": "json-helper@1300"
				},{
					"command": "json-helper.format",
					"group": "json-helper@1301"
				},{
					"command": "json-helper.fold",
					"group": "json-helper@1302",
					"when": "editorLangId == json"
				},{
					"command": "json-helper.unfold",
					"group": "json-helper@1303",
					"when": "editorLangId == json"
				},{
					"command": "json-helper.foldAll",
					"group": "json-helper@1304",
					"when": "editorLangId == json"
				},{
					"command": "json-helper.unfoldAll",
					"group": "json-helper@1305",
					"when": "editorLangId == json"
				}
			]
		}
		
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.79.0",
		"@types/glob": "^8.1.0",
		"@types/mocha": "^10.0.1",
		"@types/node": "20.2.5",
		"@typescript-eslint/eslint-plugin": "^5.59.8",
		"@typescript-eslint/parser": "^5.59.8",
		"eslint": "^8.41.0",
		"glob": "^8.1.0",
		"mocha": "^10.2.0",
		"typescript": "^5.1.3",
		"@vscode/test-electron": "^2.3.2"
	}
}
